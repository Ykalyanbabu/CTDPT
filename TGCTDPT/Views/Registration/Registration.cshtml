    $('#btnUploadDoc').on('click', function () {

        let docId = $('#ddlDocumentName').val();
        let docName = $('#ddlDocumentName option:selected').text();
        let fileInput = $('#FuFileUpload')[0];
        let rnrNo = "326011297";

        if (docId === "0") {
            alert("Please select document name");
            return;
        }

        if (fileInput.files.length === 0) {
            alert("Please select a PDF file");
            return;
        }

